{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "warn",
        "eqeqeq": "warn",
        "no-catch-shadow": "warn",
        "no-else-return": "warn",
        "no-empty-function": "off",
        "no-var": "error"
    },
    "root": true
}